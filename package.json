        "cookie-parser": "^1.4.4",
        "cron": "^1.8.2",
        "crypto": "^1.0.1",
        "csurf": "^1.11.0",
        "dateformat": "^4.5.1",
        "express": "^4.17.1",
        "express-promise-router": "^3.0.2",
        "express-session": "^1.16.1",
        "formidable": "^1.2.2",
        "fs-extra": "^8.1.0",
        "helmet": "^4.1.1",
        "http-errors": "~1.6.2",
        "ioredis": "^4.10.0",
        "jsreport-client": "^1.0.2",
        "jszip": "^3.2.2",
        "less-middleware": "^2.2.1",
