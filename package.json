{
    "name": "exa-billing",
    "version": "1.10.1",
    "private": true,
    "description": "EXA Billing Module",
    "main": "server/app.js",
    "scripts": {
        "lint": "eslint --ext .json,.js .",
        "dev": "npm run less && nodemon ./bin/www | pino-pretty",
        "dev:log": "node ./bin/www | node ./modules/logger/file-transport.js",
        "less": "cd app/skins/default && lessc index.less > index.css && cd ../dark && lessc index.less > index.css && cd ../../js/modules/multichat/stylesheets/ && lessc chat.less > chat.css",
        "start": "npm-run-all lint dev",
        "build": "npm run ci && gulp build",
        "start-production": "node ./bin/www-cluster",
        "test": "mocha --recursive test/unit",
        "ci": "npm ci && npm ci --prefix ./app",
        "prepare": "cd .. && husky install template/.husky"
    },
