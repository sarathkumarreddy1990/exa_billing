        "eslint": "^8.57.0",
        "eslint-plugin-json": "^3.1.0",
        "gulp": "^4.0.2",
        "gulp-bump": "^3.2.0",
        "gulp-chmod": "^3.1.0",
        "gulp-clean": "^0.4.0",
