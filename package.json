        "chai": "^4.3.7",
        "eslint": "^8.27.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-n": "^15.5.1",
        "eslint-plugin-promise": "^6.1.1",
        "gulp": "^4.0.2",
        "gulp-bump": "^3.2.0",
        "gulp-chmod": "^3.0.0",
        "gulp-clean": "^0.4.0",
